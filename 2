pointsForLetters = {
 1:['A', 'E', 'I', 'L', 'N', 'O', 'R', 'S', 'T', 'U'],
 2:['D', 'G']
 3:['B', 'C', 'M', 'P'],
 4:['F', 'H', 'V', 'W'],
 5:['K'],
 8:['J', 'X'],
 10:['Q', 'Z']}
word = input('Введите слово(english) ')upper()
score = 0
for i in word:
 for j in pointsForLetters.keys():
     if i in pointForLetters[j]:
        score += j
